<h1><%= @pet.name %></h1>
<h3><%= @pet.age %> years old</h3>
<h3><%= @pet.breed %></h3>
<h3><%= @pet.adoptable %></h3>
<h3><%= @pet.shelter_name %></h3>

<%= render partial: 'link', locals: { name: "Update #{@pet.name}", path: "pets/#{@pet.id}/edit", method: :get } %>
<%= render partial: 'link', locals: { name: "Delete #{@pet.name}", path: "/pets/#{@pet.id}", method: :delete } %>
